params:
  path: string
  template: string
  variables: map string any   # map of string key => value of any type
...

template = params.template
variables = params.variables

file_contents = template.render_template(variables.to_h)

success = begin
  File.write(params.path, file_contents)
  true
rescue => e
  false
end

{
  success: success
}
